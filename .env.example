# データベース 環境変数
IKZM_OICAREER_DB_URL=jdbc:mariadb://localhost:3306/oic_career_log?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC
IKZM_OICAREER_DB_USER=root
IKZM_OICAREER_DB_PASSWORD=your_password_here

# Google OAuth 2.0 環境変数
# Google Cloud Consoleで発行したクライアントIDとシークレットを設定してください
# 発行手順: https://zenn.dev/milky/articles/google-client-oauth
IKZM_OICAREER_OAUTH_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
IKZM_OICAREER_OAUTH_CLIENT_SECRET=your_client_secret_here
IKZM_OICAREER_OAUTH_REDIRECT_URI=http://localhost:8080/OICareerLog/Login
